{
  "name": "rugaard/trustpilot",
  "type": "library",
  "description": "Trustpilot API",
  "keywords": [
    "trustpilot",
    "api",
    "trustpilot api",
    "laravel",
    "package",
    "laravel package",
    "morten",
    "rugaard",
    "morten rugaard"
  ],
  "authors": [
    {
      "name": "Morten Rugaard",
      "email": "morten@rugaard.me",
      "homepage": "https://github.com/rugaard",
      "role": "Developer"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/allermedia",
  "support": {
    "issues": "https://github.com/rugaard/trustpilot/issues",
    "source": "https://github.com/rugaard/trustpilot"
  },
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "guzzlehttp/guzzle": "~6.0",
    "rugaard/oauth2-trustpilot": "*",
    "tightenco/collect": "^5.8"
  },
  "require-dev": {
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Rugaard\\Trustpilot\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rugaard\\Trustpilot\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rugaard\\Trustpilot\\Providers\\Laravel\\ServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit --colors=always"
  },
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
